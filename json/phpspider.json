{
  "contents": {
    "newduba": {
      "name": "newduba",
      "domains": [
        "newduba.cn",
        "www.newduba.cn"
      ],
      "scan_urls": [
        "https://www.newduba.cn/"
      ],
      "max_fields": 1,
      "fields": {
        "contents": {
          "name": "contents",
          "selector": "//div[@class='second_nav_tabs']//li | //ul[contains(@class,'hot_top')]/li | //ul[contains(@class,'site_cool_list')]/li",
          "repeated": true,
          "children": {
            "title": {
              "name": "title",
              "selector": "//a//text()"
            },
            "url": {
              "name": "url",
              "selector": "//a/@href"
            },
            "ico": {
              "name": "ico",
              "selector": "//a[contains(@class,'has_prev_icon')]/@style"
            }
          }
        }
      }
    }
  },
  "_content": {
    "name": "",
    "log_show": false,
    "log_file": "phpspider.log",
    "log_type": "error,debug",
    "input_encoding": null,
    "output_encoding": null,
    "tasknum": 1,
    "multiserver": false,
    "serverid": 1,
    "save_running_state": false,
    "queue_config": {
      "host": "127.0.0.1",
      "port": 6379,
      "pass": "",
      "db": 5,
      "prefix": "phpspider",
      "timeout": 30
    },
    "proxy": [],
    "interval": 1000,
    "timeout": 5,
    "max_try": 3,
    "max_depth": 0,
    "max_fields": 0,
    "user_agent": [
      "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_11_6) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/52.0.2743.116 Safari/537.36",
      "Mozilla/5.0 (iPhone; CPU iPhone OS 9_3_3 like Mac OS X) AppleWebKit/601.1.46 (KHTML, like Gecko) Version/9.0 Mobile/13G34 Safari/601.1",
      "Mozilla/5.0 (Linux; U; Android 6.0.1;zh_cn; Le X820 Build/FEXCNFN5801507014S) AppleWebKit/537.36 (KHTML, like Gecko)Version/4.0 Chrome/49.0.0.0 Mobile Safari/537.36 EUI Browser/5.8.015S"
    ],
    "client_ip": [
      "192.168.0.2",
      "192.168.0.3",
      "192.168.0.4"
    ],
    "export": {
      "type": "csv",
      "file": "./csv/phpspider.csv"
    },
    "db_config": {
      "host": "127.0.0.1",
      "port": 3306,
      "user": "root",
      "pass": "root",
      "name": "demo"
    },
    "domains": [],
    "scan_urls": [],
    "content_url_regexes": [],
    "list_url_regexes": [],
    "fields": {
      "_title": {
        "name": "_title",
        "selector_type": "xpath",
        "selector": "//title",
        "required": false,
        "repeated": false
      },
      "_ico": {
        "name": "_ico",
        "selector_type": "xpath",
        "selector": "//link[@contains(@rel,'icon')]/@href",
        "required": false,
        "repeated": false
      },
      "_keywords": {
        "name": "_keywords",
        "selector_type": "xpath",
        "selector": "//meta[@name='keywords']/@content",
        "required": false,
        "repeated": false
      },
      "_description": {
        "name": "_description",
        "selector_type": "xpath",
        "selector": "//meta[@name='description']/@content",
        "required": false,
        "repeated": false
      }
    }
  }
}