      "status": "private",
      "export_table": "novel_contents",
      "text": {
        "domains": [
          "qidian",
          "qidian.com"
        ],
        "scan_urls": [],
        "list_url_regexes": [],
        "content_slug_regexes": [],
        "fields": []
      },
      "children": [
        {
          "title": "起点中文网",
          "slug": "https://www.qidian.com/",
          "type": "novel_source",
          "status": "private",
          "export_table": "novel_contents",
          "text": {
            "scan_urls": [
              "月票榜::rank/yuepiao/",
              "畅销榜::rank/hotsales/",
              "阅读指数榜::rank/readindex/"
            ],
            "fields": [
              {
                "name": "current_page",
                "selector": "//*[contains(@class,'lbf-pagination-current')]"
              },
              {
                "name": "page_count",
                "selector": "(//*[@id='page-container']//a[contains(@class,'lbf-pagination-page')])[last()]",
                "repeated": true
              },
              {
                "name": "data",
                "selector": "//*[@id='book-img-text']//li",
                "repeated": true,
                "children": [
                  {
                    "name": "title",
                    "selector": "//*[contains(@class,'book-mid-info')]//h2//a"
                  },
                  {
                    "name": "ico",
                    "selector": "//*[contains(@class,'book-img-box')]//img/@src"
                  },
                  {
                    "name": "description",
                    "selector": "//*[contains(@class,'book-mid-info')]//*[contains(@class,'intro')]"
                  },
                  {
                    "name": "author",
                    "selector": "//*[contains(@class,'book-mid-info')]//*[contains(@class,'author')]//a",
                    "filter": "0"
                  },
                  {
                    "name": "category",
                    "selector": "//*[contains(@class,'book-mid-info')]//*[contains(@class,'author')]//a",
                    "repeated": true,
                    "filter": "array_slice(1)"
                  },
                  {
                    "name": "status",
                    "selector": "//*[contains(@class,'book-mid-info')]//*[contains(@class,'author')]//span"
                  },
                  {
                    "name": "chapters",
                    "selector": "//*[contains(@class,'book-mid-info')]//*[contains(@class,'update')]",
                    "repeated": true,
                    "children": [
                      {
                        "name": "title",
                        "selector": "//a",
                        "filter": "substr(13)"
                      },
                      {
                        "name": "url",
                        "selector": "//a/@href"
                      },
                      {
                        "name": "updated_at",
                        "selector": "//span"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        }
      ]
    }
  ],
  "_children": [
    {},
    {},
    {
